{
  "expo": {
    "name": "JAC",
    "slug": "JAC",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/assets/images/logo.png",
    "scheme": "jac",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.JAC",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show mechanics near you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "",
        "backgroundImage": "",
        "monochromeImage": ""
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.JAC",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": ["expo-router", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
